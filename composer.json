{
    "name": "nurmanhabib/laravel-menu",
    "description": "Make it easy to create navigation menus on Laravel",
    "type": "library",
    "require": {
        "laravel/framework": ">5.0",
        "nurmanhabib/navigator": "^5.0.3"
    },
    "license": "MIT",
    "keywords": [
        "laravel menu",
        "laravel navigation",
        "navigation",
        "navigator",
        "menu",
        "sidebar",
        "navbar"
    ],
    "authors": [
        {
            "name": "Habib Nurrahman",
            "email": "nurmanhabib@yahoo.co.id"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nurmanhabib\\LaravelMenu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nurmanhabib\\LaravelMenu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Nurmanhabib\\LaravelMenu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "stable"
}
